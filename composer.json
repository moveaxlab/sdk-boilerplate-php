{
    "name": "elevenlab/php-sdk-boilerplate",
    "description": "Useful Boilerplate library that can be used to build PHP SDKs for your application",
    "type": "library",
    "keywords": ["php", "sdk", "boilerplate"],
    "license": "GPL v3",
    "authors": [
        {
            "name": "Valerio Cervo",
            "email": "v.cervo.90@gmail.com"
        },
        {
            "name": "MovEax Srls",
            "email": "info@moveax.it"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.6.4",
        "guzzlehttp/guzzle": "^6.3",
        "illuminate/validation": "5.4.*",
        "illuminate/contracts": "5.4.*",
        "psr/simple-cache": "^1.0",
        "nesbot/carbon": "^2.9"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "mockery/mockery": "@stable",
        "fzaninotto/faker": "^1.8",
        "symfony/yaml": "^4.2",
        "psy/psysh": "^0.9.9",
        "chainside/validation-testvectors": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "SDK\\Boilerplate\\": "src/",
            "SDK\\Boilerplate\\Tests\\" : "tests/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "chainside/validation-testvectors",
                "version": "dev-master",
                "source": {
                    "url": "git@bitbucket.org:chainside/validation-test-vectors.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        }
    ]
}
